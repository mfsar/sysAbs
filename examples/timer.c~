#include <stddef.h>
#include <stdio.h>
#include <system/timer.h>
#include <system/system.h>

void test()
{
    printf("test\n");
}

int main(void)
{
    timer_t test_timer;
    printf("System abstraction test - POSIX implementation\n");

    system_init();
    timer_create_2(&test_timer, test,1000);
    system_start();

    return 0;
}
